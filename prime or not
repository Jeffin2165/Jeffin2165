import java.util.*;

class Prime

{

public static void main(String args[])
{ 
    Scanner sc = new Scanner(System.in); int i,c=0,n; 
    System.out.println("Enter a number");
    n= sc.nextInt(); 
    for(i=2;i<n;i++)
    { 
        if(n%i==0)
        {
            c++; 
        break;

}

}

if(c==0)
System.out.println(n+ "is a Prime number");
else System.out.println(n+ "is not a Prime number");
}}
